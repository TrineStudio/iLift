<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:id = "@+id/relativeLayout"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:background = "@drawable/main"
  >
 <ImageView
 	android:id = "@+id/lift"
 	android:layout_width = "wrap_content"
 	android:layout_height = "wrap_content"
 	android:src = "@drawable/point"
 	android:layout_alignParentBottom = "true"
 	android:layout_marginBottom = "102px"
 	android:layout_marginLeft = "13px"
 	/>
 <ImageView
 	android:id = "@+id/goods"
 	android:layout_width = "wrap_content"
 	android:layout_height = "wrap_content"
 	android:src = "@drawable/point"
	android:layout_alignTop = "@+id/lift"
	android:layout_toRightOf = "@+id/lift"
	android:layout_marginLeft = "92px"
 	/>
 <ImageView
 	android:id = "@+id/shortCut"
 	android:layout_width = "wrap_content"
 	android:layout_height = "wrap_content"
 	android:src = "@drawable/point"
	android:layout_toRightOf = "@+id/lift"
	android:layout_marginLeft = "224px"
	android:layout_marginBottom = "232px"
	android:layout_alignParentBottom = "true"

 	/> 	
 <ImageView
 	android:id = "@+id/around"
 	android:layout_width = "wrap_content"
 	android:layout_height = "wrap_content"
 	android:src = "@drawable/point"
	android:layout_above = "@+id/shortCut"
	android:layout_alignLeft = "@+id/shortCut"
	android:layout_marginBottom = "90px"

 	/> 	 	
</RelativeLayout>
